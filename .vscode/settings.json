{
    // 配置 marscode 的代码补全功能
    "marscode.codeCompletionPro": {
        "enableCodeCompletionPro": true // 启用代码补全
    },
    // 配置 marscode 的内联命令功能
    "marscode.enableInlineCommand": true, // 启用内联命令
    // 配置 marscode 的聊天语言
    "marscode.chatLanguage": "cn", // 设置为中文
    "editor.formatOnSave": true, // 保存时自动格式化
    "eslint.enable": true, // 启用 ESLint
}