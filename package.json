    "@vueuse/core": "^13.0.0",
    "ant-design-vue": "^4.2.6",
    "axios": "^1.8.1",
    "vue": "^3.5.13",
    "vue-i18n": "^10.0.5",
    "vue-router": "^4.5.0",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/node": "^22.13.10",
    "@typescript-eslint/parser": "^8.26.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-vue": "^10.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "mockjs": "^1.1.0",
    "prettier": "3.5.3",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "unplugin-vue-components": "^28.4.0",
    "vite": "^6.1.0",
    "vue-tsc": "^2.2.0"
  },
  "lint-staged": {
    "*.{js,jsx,mjs,ts,tsx}": [
